bind main Y !@git pbcopy %(commit)
bind main B !@hub browse -- commit/%(commit)
bind main D !@hub compare %(commit)...%(branch)
bind main P !@git openpr %(commit)
bind blame P !@git openpr %(commit)
bind main O !@git browse -- blob/master/%(file)

# Press R then git rebase -i will start from the commit
bind main R !git rebase -i %(commit)
bind diff R !git rebase -i %(commit)

# Support Mouse
set mouse = yes
