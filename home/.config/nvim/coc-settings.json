{
  "sourcekit.enable": true,
  "sourcekit.commandPath": "$HOME/.bin/sourcekit-lsp-wrapper",
  "grammarly.showExamples": true,
  "Lua": {
    "workspace.library": {
      "/opt/homebrew/share/nvim/runtime/lua": true,
      "/opt/homebrew/share/nvim/runtime/lua/vim": true,
      "/opt/homebrew/share/nvim/runtime/lua/vim/lsp": true
    },
    "diagnostics": {
      "globals": [ "vim" ]
    }  
  }
}
